<UserControl x:Class="DOSA_Client.Views.UploadPassportDocumentsPageView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Grid>

        <Button Content="Submit documents"
                    Command="{Binding submitDocumentsCommand}"
                    CommandParameter="Passport Status Page"
                    HorizontalAlignment="Center"
                    Width="180"
                    Height="40"
                    Background="#007ACC"
                    Foreground="White"
                    FontWeight="Bold"
                    FontSize="16"
                    Margin="0,10,0,20"/>
    </Grid>
</UserControl>
